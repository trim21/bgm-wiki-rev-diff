{
  "name": "bgm-wiki-rev-diff",
  "description": "Build your UserScript with webpack",
  "version": "0.1.7",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Trim21",
    "email": "i@trim21.me"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "run-s lint:* ",
    "format": "run-s -c format:*",
    "lint:eslint": "eslint --ext cjs,js,ts ./",
    "lint:prettier": "prettier --check .",
    "format:eslint": "eslint --fix --ext cjs,js,ts ./",
    "format:prettier": "prettier --write .",
    "anylize": "npm_config_report=true webpack --config config/webpack.config.production.js",
    "postversion": "git push",
    "build": "webpack --config config/webpack.config.production.js",
    "dev": "webpack --config config/webpack.config.dev.js",
    "lint-staged": "lint-staged -v",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/bgm-wiki-rev-diff"
  },
  "dependencies": {
    "diff": "5.0.0",
    "diff2html": "3.4.12",
    "jquery": "3.6.0",
    "lodash": "^4.17.21"
  },
  "lint-staged": {
    "*.{cjs,js,ts,json,yml,yaml}": "prettier --write",
    "*.{cjs,js,ts}": "eslint --fix"
  },
  "devDependencies": {
    "@types/diff": "5.0.1",
    "@types/greasemonkey": "^4.0.2",
    "@types/hogan.js": "^3.0.1",
    "@types/jquery": "3.5.6",
    "@types/lodash": "^4.14.175",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "ts-loader": "9.2.6",
    "typescript": "4.4.3",
    "userscript-metadata-webpack-plugin": "0.1.1",
    "webpack": "5.56.0",
    "webpack-cli": "4.8.0",
    "webpack-livereload-plugin": "3.0.2",
    "webpack-merge": "5.8.0"
  }
}
