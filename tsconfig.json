{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES6",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "allowJs": true
  },
  "include": ["src/"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
